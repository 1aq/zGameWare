class function TKDT45DI8.KDT45DI8Distance(const v1, v2: TKDT45DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT45DI8_Axis - 1 do
      Result := Result + KDT45DI8Pow(v2[i] - v1[i]);
end;

