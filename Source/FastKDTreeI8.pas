end;

class function TKDT52DI8.KDT52DI8Distance(const v1, v2: TKDT52DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT52DI8_Axis - 1 do
      Result := Result + KDT52DI8Pow(v2[i] - v1[i]);
end;

