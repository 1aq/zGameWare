class procedure TKDT42DI8.Test;
var
  TKDT42DI8_Test    : TKDT42DI8;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT42DI8_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;
begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT42DI8_Test := TKDT42DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT42DI8_Test.TestBuff, 100);
  for i := 0 to length(TKDT42DI8_Test.TestBuff) - 1 do
    for j := 0 to KDT42DI8_Axis - 1 do
        TKDT42DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT42DI8_Test.TestBuff), length(TKDT42DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT42DI8_Test.BuildKDTreeM(length(TKDT42DI8_Test.TestBuff), nil, @TKDT42DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT42DI8_Test.BuildKDTreeM(length(TKDT42DI8_Test.TestBuff), nil, TKDT42DI8_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT42DI8_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT42DI8_Test.TestBuff));
  TKDT42DI8_Test.Search(TKDT42DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT42DI8Distance(TKDT42DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT42DI8_Test.Clear;
  { kMean test }
  TKDT42DI8_Test.BuildKDTreeWithCluster(TKDT42DI8_Test.TestBuff, 10, 1, KMeanOutIndex);
  { parallel search test }
  TKDT42DI8_Test.Search(TKDT42DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if TestResultIndex[i] <> KMeanOutIndex[i] then
        errored := True;

  SetLength(TKDT42DI8_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT42DI8_Test.Clear;

  if errored then
      DoStatusNoLn('error!')
  else
      DoStatusNoLn('passed ok %dms', [GetTimeTick - t]);
  DoStatusNoLn;

  DisposeObject(TKDT42DI8_Test);
end;


function TKDT43DI8.InternalBuildKdTree(const KDSourceBufferPtr: PKDT43DI8_SourceBuffer; const PlanCount, Depth: NativeInt): PKDT43DI8_Node;
  function SortCompare(const p1, p2: PKDT43DI8_Source; const Axis: NativeInt): ShortInt;
  begin
    if p1^.Buff[Axis] = p2^.Buff[Axis] then
      begin
        if p1^.index = p2^.index then
            Result := 0
        else if p1^.index < p2^.index then
            Result := -1
        else
            Result := 1;
      end
    else if p1^.Buff[Axis] < p2^.Buff[Axis] then
        Result := -1
    else
        Result := 1;
  end;

  procedure InternalSort(const SortBuffer: PKDT43DI8_SourceBuffer; l, r: NativeInt; const Axis: NativeInt);
  var
    i, j: NativeInt;
    p, t: PKDT43DI8_Source;
  begin
    repeat
      i := l;
      j := r;
      p := SortBuffer^[(l + r) shr 1];
      repeat
        while SortCompare(SortBuffer^[i], p, Axis) < 0 do
            inc(i);
        while SortCompare(SortBuffer^[j], p, Axis) > 0 do
            Dec(j);
        if i <= j then
          begin
            if i <> j then
              begin
                t := SortBuffer^[i];
                SortBuffer^[i] := SortBuffer^[j];
                SortBuffer^[j] := t;
              end;
            inc(i);
            Dec(j);
          end;
      until i > j;
      if l < j then
          InternalSort(SortBuffer, l, j, Axis);
      l := i;
    until i >= r;
  end;

var
  m        : NativeInt;
  Axis     : NativeInt;
  kdBuffPtr: PKDT43DI8_SourceBuffer;
begin
  Result := nil;
  if PlanCount = 0 then
      Exit;

  if PlanCount = 1 then
    begin
      New(Result);
      Result^.Parent := nil;
      Result^.Right := nil;
      Result^.Left := nil;
      Result^.vec := KDSourceBufferPtr^[0];

      KDNodes[NodeCounter] := Result;
      inc(NodeCounter);
    end
  else
    begin
      Axis := Depth mod KDT43DI8_Axis;
      m := PlanCount div 2;

      kdBuffPtr := GetMemory(PlanCount * SizeOf(Pointer));
      CopyPtr(@KDSourceBufferPtr^[0], @kdBuffPtr^[0], PlanCount * SizeOf(Pointer));

      if PlanCount > 1 then
          InternalSort(@kdBuffPtr[0], 0, PlanCount - 1, Axis);

      New(Result);
      Result^.Parent := nil;
      Result^.vec := kdBuffPtr^[m];

      KDNodes[NodeCounter] := Result;
      inc(NodeCounter);

      Result^.Left := InternalBuildKdTree(@kdBuffPtr^[0], m, Depth + 1);
      if Result^.Left <> nil then
          Result^.Left^.Parent := Result;

      Result^.Right := InternalBuildKdTree(@kdBuffPtr^[m + 1], PlanCount - (m + 1), Depth + 1);
      if Result^.Right <> nil then
          Result^.Right^.Parent := Result;

      FreeMemory(kdBuffPtr);
    end;
end;

function TKDT43DI8.GetData(const index: NativeInt): PKDT43DI8_Source;
begin
  Result := @KDStoreBuff[index];
end;

constructor TKDT43DI8.Create;
begin
  inherited Create;
  NodeCounter := 0;
  RootNode := nil;
