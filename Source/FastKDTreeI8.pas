class function TKDT32DI8.KDT32DI8Distance(const v1, v2: TKDT32DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT32DI8_Axis - 1 do
      Result := Result + KDT32DI8Pow(v2[i] - v1[i]);
end;

