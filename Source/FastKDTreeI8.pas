class function TKDT35DI8.KDT35DI8Distance(const v1, v2: TKDT35DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT35DI8_Axis - 1 do
      Result := Result + KDT35DI8Pow(v2[i] - v1[i]);
end;

