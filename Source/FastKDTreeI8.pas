  i, j           : NativeInt;
  TestResultBuff : TKDT43DI8_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;
begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT43DI8_Test := TKDT43DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT43DI8_Test.TestBuff, 100);
  for i := 0 to length(TKDT43DI8_Test.TestBuff) - 1 do
    for j := 0 to KDT43DI8_Axis - 1 do
        TKDT43DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT43DI8_Test.TestBuff), length(TKDT43DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT43DI8_Test.BuildKDTreeM(length(TKDT43DI8_Test.TestBuff), nil, @TKDT43DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT43DI8_Test.BuildKDTreeM(length(TKDT43DI8_Test.TestBuff), nil, TKDT43DI8_Test.Test_BuildM);
