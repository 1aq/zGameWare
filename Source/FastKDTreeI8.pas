class function TKDT53DI8.KDT53DI8Distance(const v1, v2: TKDT53DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT53DI8_Axis - 1 do
      Result := Result + KDT53DI8Pow(v2[i] - v1[i]);
end;

