class function TKDT4DI8.KDT4DI8Distance(const v1, v2: TKDT4DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT4DI8_Axis - 1 do
      Result := Result + KDT4DI8Pow(v2[i] - v1[i]);
end;

