class function TKDT16DI8.KDT16DI8Distance(const v1, v2: TKDT16DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT16DI8_Axis - 1 do
      Result := Result + KDT16DI8Pow(v2[i] - v1[i]);
end;

