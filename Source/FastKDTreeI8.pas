class function TKDT42DI8.KDT42DI8Distance(const v1, v2: TKDT42DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT42DI8_Axis - 1 do
      Result := Result + KDT42DI8Pow(v2[i] - v1[i]);
end;

