class function TKDT18DI8.KDT18DI8Distance(const v1, v2: TKDT18DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT18DI8_Axis - 1 do
      Result := Result + KDT18DI8Pow(v2[i] - v1[i]);
end;

