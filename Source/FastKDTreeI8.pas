class procedure TKDT60DI8.Test;
var
  TKDT60DI8_Test    : TKDT60DI8;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT60DI8_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;
begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT60DI8_Test := TKDT60DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT60DI8_Test.TestBuff, 100);
  for i := 0 to length(TKDT60DI8_Test.TestBuff) - 1 do
    for j := 0 to KDT60DI8_Axis - 1 do
        TKDT60DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT60DI8_Test.TestBuff), length(TKDT60DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT60DI8_Test.BuildKDTreeM(length(TKDT60DI8_Test.TestBuff), nil, @TKDT60DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT60DI8_Test.BuildKDTreeM(length(TKDT60DI8_Test.TestBuff), nil, TKDT60DI8_Test.Test_BuildM);
