class function TKDT33DI8.KDT33DI8Distance(const v1, v2: TKDT33DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT33DI8_Axis - 1 do
      Result := Result + KDT33DI8Pow(v2[i] - v1[i]);
end;

