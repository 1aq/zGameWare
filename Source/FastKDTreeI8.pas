class function TKDT8DI8.KDT8DI8Distance(const v1, v2: TKDT8DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT8DI8_Axis - 1 do
      Result := Result + KDT8DI8Pow(v2[i] - v1[i]);
end;

