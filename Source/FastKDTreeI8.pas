class function TKDT56DI8.KDT56DI8Vec(const s: SystemString): TKDT56DI8_Vec;
var
  t          : TTextParsing;
  SplitOutput: TArrayPascalString;
  i, j       : NativeInt;
begin
  for i := 0 to KDT56DI8_Axis - 1 do
      Result[i] := 0;

  t := TTextParsing.Create(s, tsText, nil);
  if t.SplitChar(1, ', ', '', SplitOutput) > 0 then
    begin
      j := 0;
      for i := 0 to length(SplitOutput) - 1 do
        if umlGetNumTextType(SplitOutput[i]) <> ntUnknow then
          begin
            Result[j] := umlStrToInt(SplitOutput[i], 0);
            inc(j);
            if j >= KDT56DI8_Axis then
                break;
          end;
    end;
  DisposeObject(t);
end;

class function TKDT56DI8.KDT56DI8Vec(const v: TKDT56DI8_Vec): SystemString;
var
  i: NativeInt;
begin
  Result := '';
  for i := 0 to KDT56DI8_Axis - 1 do
    begin
      if i > 0 then
          Result := Result + ',';
      Result := Result + umlIntToStr(v[i]);
    end;
end;

class function TKDT56DI8.KDT56DI8Pow(const v: TKDT56DI8_VecType): Double;
begin
  Result := v * v;
end;

class function TKDT56DI8.KDT56DI8Distance(const v1, v2: TKDT56DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT56DI8_Axis - 1 do
      Result := Result + KDT56DI8Pow(v2[i] - v1[i]);
end;

