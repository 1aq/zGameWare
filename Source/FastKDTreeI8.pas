class function TKDT49DI8.KDT49DI8Distance(const v1, v2: TKDT49DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT49DI8_Axis - 1 do
      Result := Result + KDT49DI8Pow(v2[i] - v1[i]);
end;

