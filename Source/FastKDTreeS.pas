end;

class function TKDT35DS.KDT35DSDistance(const v1, v2: TKDT35DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT35DS_Axis - 1 do
      Result := Result + KDT35DSPow(v2[i] - v1[i]);
end;

