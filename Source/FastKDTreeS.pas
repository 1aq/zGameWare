end;

class function TKDT55DS.KDT55DSDistance(const v1, v2: TKDT55DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT55DS_Axis - 1 do
      Result := Result + KDT55DSPow(v2[i] - v1[i]);
end;

