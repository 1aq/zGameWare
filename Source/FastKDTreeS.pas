end;

class function TKDT17DS.KDT17DSDistance(const v1, v2: TKDT17DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT17DS_Axis - 1 do
      Result := Result + KDT17DSPow(v2[i] - v1[i]);
end;

