class function TKDT34DS.KDT34DSDistance(const v1, v2: TKDT34DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT34DS_Axis - 1 do
      Result := Result + KDT34DSPow(v2[i] - v1[i]);
end;

