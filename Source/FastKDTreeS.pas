class function TKDT47DS.KDT47DSDistance(const v1, v2: TKDT47DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT47DS_Axis - 1 do
      Result := Result + KDT47DSPow(v2[i] - v1[i]);
end;

