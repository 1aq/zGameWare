class function TKDT3DS.KDT3DSDistance(const v1, v2: TKDT3DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT3DS_Axis - 1 do
      Result := Result + KDT3DSPow(v2[i] - v1[i]);
end;

