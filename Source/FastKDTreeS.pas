class function TKDT9DS.KDT9DSDistance(const v1, v2: TKDT9DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT9DS_Axis - 1 do
      Result := Result + KDT9DSPow(v2[i] - v1[i]);
end;

