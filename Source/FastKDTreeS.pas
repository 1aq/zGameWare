class function TKDT4DS.KDT4DSDistance(const v1, v2: TKDT4DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT4DS_Axis - 1 do
      Result := Result + KDT4DSPow(v2[i] - v1[i]);
end;

