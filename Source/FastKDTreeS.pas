end;

class function TKDT16DS.KDT16DSDistance(const v1, v2: TKDT16DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT16DS_Axis - 1 do
      Result := Result + KDT16DSPow(v2[i] - v1[i]);
end;

