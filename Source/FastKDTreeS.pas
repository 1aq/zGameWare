class function TKDT60DS.KDT60DSDistance(const v1, v2: TKDT60DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT60DS_Axis - 1 do
      Result := Result + KDT60DSPow(v2[i] - v1[i]);
end;

