end;

class function TKDT36DS.KDT36DSDistance(const v1, v2: TKDT36DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT36DS_Axis - 1 do
      Result := Result + KDT36DSPow(v2[i] - v1[i]);
end;

