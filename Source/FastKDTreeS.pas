class function TKDT30DS.KDT30DSDistance(const v1, v2: TKDT30DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT30DS_Axis - 1 do
      Result := Result + KDT30DSPow(v2[i] - v1[i]);
end;

