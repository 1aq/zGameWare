end;

class function TKDT11DS.KDT11DSDistance(const v1, v2: TKDT11DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT11DS_Axis - 1 do
      Result := Result + KDT11DSPow(v2[i] - v1[i]);
end;

