class function TKDT28DS.KDT28DSDistance(const v1, v2: TKDT28DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT28DS_Axis - 1 do
      Result := Result + KDT28DSPow(v2[i] - v1[i]);
end;

