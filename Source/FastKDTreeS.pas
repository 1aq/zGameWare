end;

class function TKDT5DS.KDT5DSDistance(const v1, v2: TKDT5DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT5DS_Axis - 1 do
      Result := Result + KDT5DSPow(v2[i] - v1[i]);
end;

