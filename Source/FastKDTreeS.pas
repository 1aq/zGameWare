end;

class function TKDT23DS.KDT23DSDistance(const v1, v2: TKDT23DS_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT23DS_Axis - 1 do
      Result := Result + KDT23DSPow(v2[i] - v1[i]);
end;

