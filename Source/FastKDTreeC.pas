end;

class function TKDT15DC.KDT15DCDistance(const v1, v2: TKDT15DC_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT15DC_Axis - 1 do
      Result := Result + KDT15DCPow(v2[i] - v1[i]);
end;

