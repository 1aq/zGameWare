end;

class function TKDT4DC.KDT4DCDistance(const v1, v2: TKDT4DC_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT4DC_Axis - 1 do
      Result := Result + KDT4DCPow(v2[i] - v1[i]);
end;

