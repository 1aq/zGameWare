end;

class function TKDT23DC.KDT23DCDistance(const v1, v2: TKDT23DC_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT23DC_Axis - 1 do
      Result := Result + KDT23DCPow(v2[i] - v1[i]);
end;

