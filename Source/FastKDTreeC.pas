end;

class function TKDT61DC.KDT61DCDistance(const v1, v2: TKDT61DC_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT61DC_Axis - 1 do
      Result := Result + KDT61DCPow(v2[i] - v1[i]);
end;

