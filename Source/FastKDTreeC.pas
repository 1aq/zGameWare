class function TKDT10DC.KDT10DCDistance(const v1, v2: TKDT10DC_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT10DC_Axis - 1 do
      Result := Result + KDT10DCPow(v2[i] - v1[i]);
end;

