class function TKDT50DC.KDT50DCDistance(const v1, v2: TKDT50DC_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT50DC_Axis - 1 do
      Result := Result + KDT50DCPow(v2[i] - v1[i]);
end;

