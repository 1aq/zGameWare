end;

class function TKDT55DC.KDT55DCDistance(const v1, v2: TKDT55DC_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT55DC_Axis - 1 do
      Result := Result + KDT55DCPow(v2[i] - v1[i]);
end;

