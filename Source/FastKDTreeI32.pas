class function TKDT60DI32.KDT60DI32Distance(const v1, v2: TKDT60DI32_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT60DI32_Axis - 1 do
      Result := Result + KDT60DI32Pow(v2[i] - v1[i]);
end;

