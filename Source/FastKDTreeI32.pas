class function TKDT56DI32.KDT56DI32Vec(const s: SystemString): TKDT56DI32_Vec;
var
  t          : TTextParsing;
  SplitOutput: TArrayPascalString;
  i, j       : NativeInt;
begin
  for i := 0 to KDT56DI32_Axis - 1 do
      Result[i] := 0;

  t := TTextParsing.Create(s, tsText, nil);
  if t.SplitChar(1, ', ', '', SplitOutput) > 0 then
    begin
      j := 0;
      for i := 0 to length(SplitOutput) - 1 do
        if umlGetNumTextType(SplitOutput[i]) <> ntUnknow then
          begin
            Result[j] := umlStrToInt(SplitOutput[i], 0);
            inc(j);
            if j >= KDT56DI32_Axis then
                break;
          end;
    end;
  DisposeObject(t);
end;

class function TKDT56DI32.KDT56DI32Vec(const v: TKDT56DI32_Vec): SystemString;
var
  i: NativeInt;
begin
  Result := '';
  for i := 0 to KDT56DI32_Axis - 1 do
    begin
      if i > 0 then
          Result := Result + ',';
      Result := Result + umlIntToStr(v[i]);
    end;
end;

class function TKDT56DI32.KDT56DI32Pow(const v: TKDT56DI32_VecType): Double;
begin
  Result := v * v;
end;

class function TKDT56DI32.KDT56DI32Distance(const v1, v2: TKDT56DI32_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT56DI32_Axis - 1 do
      Result := Result + KDT56DI32Pow(v2[i] - v1[i]);
end;

