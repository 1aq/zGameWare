class function TKDT49DI32.KDT49DI32Distance(const v1, v2: TKDT49DI32_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT49DI32_Axis - 1 do
      Result := Result + KDT49DI32Pow(v2[i] - v1[i]);
end;

