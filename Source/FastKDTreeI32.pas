class function TKDT42DI32.KDT42DI32Vec(const s: SystemString): TKDT42DI32_Vec;
var
  t          : TTextParsing;
  SplitOutput: TArrayPascalString;
  i, j       : NativeInt;
begin
  for i := 0 to KDT42DI32_Axis - 1 do
      Result[i] := 0;

  t := TTextParsing.Create(s, tsText, nil);
  if t.SplitChar(1, ', ', '', SplitOutput) > 0 then
    begin
      j := 0;
      for i := 0 to length(SplitOutput) - 1 do
        if umlGetNumTextType(SplitOutput[i]) <> ntUnknow then
          begin
            Result[j] := umlStrToInt(SplitOutput[i], 0);
            inc(j);
            if j >= KDT42DI32_Axis then
                break;
          end;
    end;
  DisposeObject(t);
end;

class function TKDT42DI32.KDT42DI32Vec(const v: TKDT42DI32_Vec): SystemString;
var
  i: NativeInt;
begin
  Result := '';
  for i := 0 to KDT42DI32_Axis - 1 do
    begin
      if i > 0 then
          Result := Result + ',';
      Result := Result + umlIntToStr(v[i]);
    end;
end;

