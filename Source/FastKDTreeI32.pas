class function TKDT32DI32.KDT32DI32Distance(const v1, v2: TKDT32DI32_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT32DI32_Axis - 1 do
      Result := Result + KDT32DI32Pow(v2[i] - v1[i]);
end;

