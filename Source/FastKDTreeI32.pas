class function TKDT15DI32.KDT15DI32Distance(const v1, v2: TKDT15DI32_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT15DI32_Axis - 1 do
      Result := Result + KDT15DI32Pow(v2[i] - v1[i]);
end;

