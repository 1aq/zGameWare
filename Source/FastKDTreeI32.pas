class function TKDT52DI32.KDT52DI32Distance(const v1, v2: TKDT52DI32_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT52DI32_Axis - 1 do
      Result := Result + KDT52DI32Pow(v2[i] - v1[i]);
end;

