end;

class function TKDT26DI32.KDT26DI32Distance(const v1, v2: TKDT26DI32_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT26DI32_Axis - 1 do
      Result := Result + KDT26DI32Pow(v2[i] - v1[i]);
end;

