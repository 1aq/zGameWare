end;

class function TKDT43DI32.KDT43DI32Distance(const v1, v2: TKDT43DI32_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT43DI32_Axis - 1 do
      Result := Result + KDT43DI32Pow(v2[i] - v1[i]);
end;

