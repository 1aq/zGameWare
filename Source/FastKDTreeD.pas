class function TKDT15DD.KDT15DDDistance(const v1, v2: TKDT15DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT15DD_Axis - 1 do
      Result := Result + KDT15DDPow(v2[i] - v1[i]);
end;

