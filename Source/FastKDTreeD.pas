  Result := v * v;
end;

class function TKDT10DD.KDT10DDDistance(const v1, v2: TKDT10DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT10DD_Axis - 1 do
      Result := Result + KDT10DDPow(v2[i] - v1[i]);
end;

