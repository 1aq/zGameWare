  Result := v * v;
end;

class function TKDT54DD.KDT54DDDistance(const v1, v2: TKDT54DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT54DD_Axis - 1 do
      Result := Result + KDT54DDPow(v2[i] - v1[i]);
end;

