end;

class function TKDT50DD.KDT50DDDistance(const v1, v2: TKDT50DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT50DD_Axis - 1 do
      Result := Result + KDT50DDPow(v2[i] - v1[i]);
end;

