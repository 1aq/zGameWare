class function TKDT28DD.KDT28DDDistance(const v1, v2: TKDT28DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT28DD_Axis - 1 do
      Result := Result + KDT28DDPow(v2[i] - v1[i]);
end;

