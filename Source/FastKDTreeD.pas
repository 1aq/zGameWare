end;

class function TKDT57DD.KDT57DDDistance(const v1, v2: TKDT57DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT57DD_Axis - 1 do
      Result := Result + KDT57DDPow(v2[i] - v1[i]);
end;

