class function TKDT3DD.KDT3DDDistance(const v1, v2: TKDT3DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT3DD_Axis - 1 do
      Result := Result + KDT3DDPow(v2[i] - v1[i]);
end;

