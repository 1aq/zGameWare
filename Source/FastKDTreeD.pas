      end);
    {$ENDIF FPC}
  finally
      GlobalMemoryHook := True;
  end;
end;

{$ELSE parallel}


var
  i: NativeInt;
  p: PKDT61DD_Node;
begin
  if length(inBuff) <> length(OutIndex) then
