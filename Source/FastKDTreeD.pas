class function TKDT43DD.KDT43DDDistance(const v1, v2: TKDT43DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT43DD_Axis - 1 do
      Result := Result + KDT43DDPow(v2[i] - v1[i]);
end;

