end;

class function TKDT35DD.KDT35DDDistance(const v1, v2: TKDT35DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT35DD_Axis - 1 do
      Result := Result + KDT35DDPow(v2[i] - v1[i]);
end;

