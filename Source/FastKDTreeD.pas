end;

class function TKDT61DD.KDT61DDDistance(const v1, v2: TKDT61DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT61DD_Axis - 1 do
      Result := Result + KDT61DDPow(v2[i] - v1[i]);
end;

