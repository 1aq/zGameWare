class function TKDT30DD.KDT30DDDistance(const v1, v2: TKDT30DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT30DD_Axis - 1 do
      Result := Result + KDT30DDPow(v2[i] - v1[i]);
end;

