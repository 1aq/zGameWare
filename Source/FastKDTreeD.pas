class function TKDT11DD.KDT11DDDistance(const v1, v2: TKDT11DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT11DD_Axis - 1 do
      Result := Result + KDT11DDPow(v2[i] - v1[i]);
end;

