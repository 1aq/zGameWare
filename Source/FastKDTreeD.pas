class function TKDT49DD.KDT49DDDistance(const v1, v2: TKDT49DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT49DD_Axis - 1 do
      Result := Result + KDT49DDPow(v2[i] - v1[i]);
end;

