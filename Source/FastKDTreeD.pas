class function TKDT16DD.KDT16DDDistance(const v1, v2: TKDT16DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT16DD_Axis - 1 do
      Result := Result + KDT16DDPow(v2[i] - v1[i]);
end;

