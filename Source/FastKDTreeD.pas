end;

class function TKDT29DD.KDT29DDDistance(const v1, v2: TKDT29DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT29DD_Axis - 1 do
      Result := Result + KDT29DDPow(v2[i] - v1[i]);
end;

