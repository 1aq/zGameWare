class function TKDT41DD.KDT41DDDistance(const v1, v2: TKDT41DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT41DD_Axis - 1 do
      Result := Result + KDT41DDPow(v2[i] - v1[i]);
end;

