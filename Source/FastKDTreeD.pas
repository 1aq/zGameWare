end;

class function TKDT25DD.KDT25DDDistance(const v1, v2: TKDT25DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT25DD_Axis - 1 do
      Result := Result + KDT25DDPow(v2[i] - v1[i]);
end;

