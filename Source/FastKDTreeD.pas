end;

class function TKDT48DD.KDT48DDDistance(const v1, v2: TKDT48DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT48DD_Axis - 1 do
      Result := Result + KDT48DDPow(v2[i] - v1[i]);
end;

