class function TKDT47DD.KDT47DDDistance(const v1, v2: TKDT47DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT47DD_Axis - 1 do
      Result := Result + KDT47DDPow(v2[i] - v1[i]);
end;

