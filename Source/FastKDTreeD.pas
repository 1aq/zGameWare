end;

class function TKDT5DD.KDT5DDDistance(const v1, v2: TKDT5DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT5DD_Axis - 1 do
      Result := Result + KDT5DDPow(v2[i] - v1[i]);
end;

