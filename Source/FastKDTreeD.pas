class function TKDT60DD.KDT60DDDistance(const v1, v2: TKDT60DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT60DD_Axis - 1 do
      Result := Result + KDT60DDPow(v2[i] - v1[i]);
end;

