class function TKDT62DD.KDT62DDDistance(const v1, v2: TKDT62DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT62DD_Axis - 1 do
      Result := Result + KDT62DDPow(v2[i] - v1[i]);
end;

