end;

class function TKDT56DD.KDT56DDDistance(const v1, v2: TKDT56DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT56DD_Axis - 1 do
      Result := Result + KDT56DDPow(v2[i] - v1[i]);
end;

