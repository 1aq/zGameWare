end;

class function TKDT24DD.KDT24DDDistance(const v1, v2: TKDT24DD_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT24DD_Axis - 1 do
      Result := Result + KDT24DDPow(v2[i] - v1[i]);
end;

