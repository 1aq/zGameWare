class function TKDT52DI64.KDT52DI64Distance(const v1, v2: TKDT52DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT52DI64_Axis - 1 do
      Result := Result + KDT52DI64Pow(v2[i] - v1[i]);
end;

