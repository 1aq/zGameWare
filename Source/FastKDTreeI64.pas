class function TKDT8DI64.KDT8DI64Distance(const v1, v2: TKDT8DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT8DI64_Axis - 1 do
      Result := Result + KDT8DI64Pow(v2[i] - v1[i]);
end;

