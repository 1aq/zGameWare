class function TKDT32DI64.KDT32DI64Distance(const v1, v2: TKDT32DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT32DI64_Axis - 1 do
      Result := Result + KDT32DI64Pow(v2[i] - v1[i]);
end;

