      FillPtrByte(@KDStoreBuff[i].Buff[0], SizeOf(TKDT48DI64_Vec), 0);
      OnTrigger(i, KDStoreBuff[i], Data);
      inc(i);
    end;

  j := PlanCount;

  RootNode := InternalBuildKdTree(@KDBuff[0], j, 0);
end;
