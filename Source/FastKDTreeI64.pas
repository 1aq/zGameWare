class function TKDT49DI64.KDT49DI64Distance(const v1, v2: TKDT49DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT49DI64_Axis - 1 do
      Result := Result + KDT49DI64Pow(v2[i] - v1[i]);
end;

