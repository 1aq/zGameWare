end;

class function TKDT15DI64.KDT15DI64Distance(const v1, v2: TKDT15DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT15DI64_Axis - 1 do
      Result := Result + KDT15DI64Pow(v2[i] - v1[i]);
end;

