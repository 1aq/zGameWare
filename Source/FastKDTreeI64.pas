class function TKDT42DI64.KDT42DI64Distance(const v1, v2: TKDT42DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT42DI64_Axis - 1 do
      Result := Result + KDT42DI64Pow(v2[i] - v1[i]);
end;

