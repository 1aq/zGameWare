class function TKDT35DI64.KDT35DI64Distance(const v1, v2: TKDT35DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT35DI64_Axis - 1 do
      Result := Result + KDT35DI64Pow(v2[i] - v1[i]);
end;

