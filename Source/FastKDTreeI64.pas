  Result := v * v;
end;

class function TKDT56DI64.KDT56DI64Distance(const v1, v2: TKDT56DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT56DI64_Axis - 1 do
      Result := Result + KDT56DI64Pow(v2[i] - v1[i]);
end;

