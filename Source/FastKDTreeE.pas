class function TKDT9DE.KDT9DEDistance(const v1, v2: TKDT9DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT9DE_Axis - 1 do
      Result := Result + KDT9DEPow(v2[i] - v1[i]);
end;

