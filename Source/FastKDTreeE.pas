class function TKDT17DE.KDT17DEDistance(const v1, v2: TKDT17DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT17DE_Axis - 1 do
      Result := Result + KDT17DEPow(v2[i] - v1[i]);
end;

