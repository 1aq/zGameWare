class function TKDT23DE.KDT23DEDistance(const v1, v2: TKDT23DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT23DE_Axis - 1 do
      Result := Result + KDT23DEPow(v2[i] - v1[i]);
end;

