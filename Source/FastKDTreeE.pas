end;

class function TKDT34DE.KDT34DEDistance(const v1, v2: TKDT34DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT34DE_Axis - 1 do
      Result := Result + KDT34DEPow(v2[i] - v1[i]);
end;

