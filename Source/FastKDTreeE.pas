class function TKDT53DE.KDT53DEDistance(const v1, v2: TKDT53DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT53DE_Axis - 1 do
      Result := Result + KDT53DEPow(v2[i] - v1[i]);
end;

