class function TKDT60DE.KDT60DEDistance(const v1, v2: TKDT60DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT60DE_Axis - 1 do
      Result := Result + KDT60DEPow(v2[i] - v1[i]);
end;

