end;

class function TKDT29DE.KDT29DEDistance(const v1, v2: TKDT29DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT29DE_Axis - 1 do
      Result := Result + KDT29DEPow(v2[i] - v1[i]);
end;

