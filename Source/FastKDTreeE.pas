class function TKDT50DE.KDT50DEDistance(const v1, v2: TKDT50DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT50DE_Axis - 1 do
      Result := Result + KDT50DEPow(v2[i] - v1[i]);
end;

