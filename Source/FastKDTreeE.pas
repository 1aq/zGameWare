class function TKDT5DE.KDT5DEDistance(const v1, v2: TKDT5DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT5DE_Axis - 1 do
      Result := Result + KDT5DEPow(v2[i] - v1[i]);
end;

