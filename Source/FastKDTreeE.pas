end;

class function TKDT35DE.KDT35DEDistance(const v1, v2: TKDT35DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT35DE_Axis - 1 do
      Result := Result + KDT35DEPow(v2[i] - v1[i]);
end;

