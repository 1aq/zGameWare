begin
  Result := v * v;
end;

class function TKDT15DE.KDT15DEDistance(const v1, v2: TKDT15DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT15DE_Axis - 1 do
      Result := Result + KDT15DEPow(v2[i] - v1[i]);
end;

