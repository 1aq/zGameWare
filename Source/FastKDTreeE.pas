begin
  Result := v * v;
end;

class function TKDT49DE.KDT49DEDistance(const v1, v2: TKDT49DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT49DE_Axis - 1 do
      Result := Result + KDT49DEPow(v2[i] - v1[i]);
end;

