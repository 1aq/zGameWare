  Result := v * v;
end;

class function TKDT44DE.KDT44DEDistance(const v1, v2: TKDT44DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT44DE_Axis - 1 do
      Result := Result + KDT44DEPow(v2[i] - v1[i]);
end;

