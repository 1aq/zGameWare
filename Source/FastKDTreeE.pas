class function TKDT28DE.KDT28DEDistance(const v1, v2: TKDT28DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT28DE_Axis - 1 do
      Result := Result + KDT28DEPow(v2[i] - v1[i]);
end;

