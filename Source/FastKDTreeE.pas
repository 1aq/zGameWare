end;

class function TKDT61DE.KDT61DEDistance(const v1, v2: TKDT61DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT61DE_Axis - 1 do
      Result := Result + KDT61DEPow(v2[i] - v1[i]);
end;

