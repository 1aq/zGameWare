class function TKDT55DE.KDT55DEDistance(const v1, v2: TKDT55DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT55DE_Axis - 1 do
      Result := Result + KDT55DEPow(v2[i] - v1[i]);
end;

