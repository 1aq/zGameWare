end;

class function TKDT54DE.KDT54DEDistance(const v1, v2: TKDT54DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT54DE_Axis - 1 do
      Result := Result + KDT54DEPow(v2[i] - v1[i]);
end;

