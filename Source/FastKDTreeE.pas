class function TKDT16DE.KDT16DEDistance(const v1, v2: TKDT16DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT16DE_Axis - 1 do
      Result := Result + KDT16DEPow(v2[i] - v1[i]);
end;

