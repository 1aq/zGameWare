class function TKDT4DE.KDT4DEDistance(const v1, v2: TKDT4DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT4DE_Axis - 1 do
      Result := Result + KDT4DEPow(v2[i] - v1[i]);
end;

