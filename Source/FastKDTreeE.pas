class function TKDT11DE.KDT11DEDistance(const v1, v2: TKDT11DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT11DE_Axis - 1 do
      Result := Result + KDT11DEPow(v2[i] - v1[i]);
end;

