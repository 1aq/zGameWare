class function TKDT56DE.KDT56DEDistance(const v1, v2: TKDT56DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT56DE_Axis - 1 do
      Result := Result + KDT56DEPow(v2[i] - v1[i]);
end;

