end;

class function TKDT62DE.KDT62DEDistance(const v1, v2: TKDT62DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT62DE_Axis - 1 do
      Result := Result + KDT62DEPow(v2[i] - v1[i]);
end;

