  Result := v * v;
end;

class function TKDT48DE.KDT48DEDistance(const v1, v2: TKDT48DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT48DE_Axis - 1 do
      Result := Result + KDT48DEPow(v2[i] - v1[i]);
end;

