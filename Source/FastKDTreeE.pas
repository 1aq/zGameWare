end;

class function TKDT10DE.KDT10DEDistance(const v1, v2: TKDT10DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT10DE_Axis - 1 do
      Result := Result + KDT10DEPow(v2[i] - v1[i]);
end;

