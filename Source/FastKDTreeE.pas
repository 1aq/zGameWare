class function TKDT30DE.KDT30DEDistance(const v1, v2: TKDT30DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT30DE_Axis - 1 do
      Result := Result + KDT30DEPow(v2[i] - v1[i]);
end;

