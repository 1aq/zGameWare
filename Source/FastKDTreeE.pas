class function TKDT3DE.KDT3DEDistance(const v1, v2: TKDT3DE_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT3DE_Axis - 1 do
      Result := Result + KDT3DEPow(v2[i] - v1[i]);
end;

